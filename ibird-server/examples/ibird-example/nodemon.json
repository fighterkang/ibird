{
  "restartable": "rs",
  "ignore": [
    ".git",
    ".idea",
    "logs",
    "public",
    "clients",
    "node_modules/**/node_modules"
  ],
  "verbose": false,
  "execMap": {
    "js": "node --harmony"
  },
  "events": {
    "restart": "osascript -e 'display notification \"App restarted due to:\n'$FILENAME'\" with title \"nodemon\"'"
  },
  "watch": [
    "modules/",
    "menu.js",
    "index.js",
    "package.json"
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "js json"
}